@model IEnumerable<Classification>

<h3>Classification List</h3>
<a class="btn btn-primary" href="/Classification/Create">Create</a>
<br />
<table class="table table-hover" width="100%">
    <tr>
        <td>Name</td>
        <td>Description</td>
        <td>createdtime</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Description</td>
            <td>@item.createdtime</td>
            <td>
                <a class="btn btn-success" href="/Classification/Edit/@item.Id">Edit</a>
                <a class="btn btn-danger" href="/Classification/Delete/@item.Id">Delete</a>

            </td>
        </tr>
    }
</table>
