@model IEnumerable<Equipment>

@{
    ViewBag.PageTitle = "Equipment Management";
    ViewBag.CardTitle = " Equipments";
    Layout = "~/Views/Shared/_Dashboard.cshtml";
}
@* <partial name="_Toaster" />*@
<a class="btn btn-primary" href="/Equipment/Create">Create</a>
<br />
<table class="table table-hover" width="100%">
    <tr>
        <td>Name</td>
        <td>NameAr</td>
        <td>EquipmentCode</td>
        <td>Manufacturer</td>
        <td>Agent</td>
        <td>Cost</td>
        <td>createdtime</td>
        <td>Category</td>
        <td>Family</td>
        <td>Class</td>
        <td>Inst Date</td>
        <td>Department</td>
        <td>Location</td>
        <td>Note</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.NameAr</td>
            <td>@item.EquipmentCode</td>
            <td>@item.Manufacturer</td>
            <td>@item.Agent</td>
            <td>@item.Cost</td>
            <td>@item.createdtime</td>
            <td>@item.Categories.Name</td>
            <td>@item.EquipmentsFamily.Name</td>
            <td>@item.Eqclasses.Name</td>
            <td>@item.installedtime</td>
            <td>@item.Departments.Name</td>
            <td>@item.Locations.Name</td>
            <td>@item.Note</td>
            <td>
                <a class="btn btn-success" href="/Equipment/Edit/@item.Id">Edit</a>
                <a class="btn btn-danger" href="/Equipment/Delete/@item.Id">Delete</a>

            </td>
        </tr>
    }
</table>
